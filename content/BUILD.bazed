tsLibrary("setup_src", {
  srcs: ["setup.ts"],
});

nodeEntry("setup", {
  main: "setup.js",
  deps: [":setup_src", "//base/node"],
});

fileSet("py", {
  srcs: glob("python/**/*.*"),
  copy: true,
});

fileSet("node", {
  srcs: glob("node/**/*.*"),
  copy: true,
});

fileSet("web", {
  srcs: glob("web/**/*.*"),
  copy: true,
});
