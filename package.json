{
  "name": "@nersent/bazed",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "bazed:clean": "rimraf build",
    "wasm": "wasm-pack build ./core --target nodejs --no-default-features --features=bindings_wasm",
    "pyi": "cargo run --bin pyi --no-default-features --features=bindings_py --features=polars",
    "proto": "pnpm bazed run //tools:proto --verbose -- gen cli/compiler/*.proto cli/compiler"
  },
